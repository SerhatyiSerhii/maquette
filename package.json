{
  "name": "maquette",
  "version": "1.0.0",
  "description": "Top 10",
  "main": "index.js",
  "scripts": {
    "build": "npm run scss:build && npm run jquery",
    "scss": "sass --watch scss/styles.scss styles.css",
    "scss:build": "sass --no-source-map scss/styles.scss styles.css --style compressed",
    "jquery": "rm -rf generated-scripts && mkdir generated-scripts && cp node_modules/jquery/dist/jquery.min.js generated-scripts/"

  },
